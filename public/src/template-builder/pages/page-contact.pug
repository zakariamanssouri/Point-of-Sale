extends template

block pageVars
	- var pageTitle = "Page Contact"
	- var pageSubTitle = ""
	- var description = "The standard font Material Design uses is Roboto. We have included the font files with our framework."
	- var activeParentMenu = "pages"
	- var activeMenu = "page-contact"

append footerConfig
	//- // - var footerType = 'footer-fixed'

append header
	- var contentBreadcrumbs = false
	- var contentHeader = true

block breadcrumbs
	+breadcrumbs([{url:"index.html",name:"Home"}, {url:"#",name:"Apps"}, {name:"Page Contact"}])

//- Include page content in page block
append content
	include ../contents/page-contact.html
	include ../contents/sidebar/right-sidebar.html

//- Vendor css
//- Add vendor specific CSS
append vendorcss
	link(rel='stylesheet', type='text/css', href=app_assets_path + '/vendors/flag-icon/css/flag-icon.min.css')
	link(rel='stylesheet', type='text/css', href=app_assets_path + '/fonts/fontawesome/css/all.min.css')


//- Page CSS
//------------------------------
//- Add custom page specific CSS
append pagecss
	link(rel='stylesheet', type='text/css', href=app_assets_path + '/css'+rtl+'/pages/page-contact.css')


//- Vendor JS
//------------------------------
//- Add vendor specific JS


//- Page JS
//------------------------------
//- Add custom page specific JS
append pagejs
	script(src=app_assets_path + '/js/scripts/page-contact.js')