extends template

block pageVars
	- var pageTitle = "Animations"
	- var pageSubTitle = ""
	- var description = "The standard font Material Design uses is Roboto. We have included the font files with our framework.."
	- var activeParentMenu = "css"
	- var activeMenu = "css-animations"

block breadcrumbs
	+breadcrumbs([{url:"index.html",name:"Home"}, {url:"#",name:"CSS"}, {name:"Animations"}])

//- Include page content in page block
append content
	include ../contents/css-animations.html
	include ../contents/sidebar/right-sidebar.html
	include ../contents/sidebar/fab-menu.html

//- Vendor CSS
//------------------------------
//- Add Vendor specific CSS
append vendorcss
	link(rel="stylesheet", type='text/css',  href=app_assets_path + '/vendors/animate-css/animate.css')

//- Page JS
//------------------------------
//- Add custom page specific JS
append pagejs
	script(src=app_assets_path + '/js/scripts/css-animation.js')
