extends template

block pageVars
	- var pageTitle = "User Profile Page"
	- var pageSubTitle = ""
	- var description = "The standard font Material Design uses is Roboto. We have included the font files with our framework.."
	- var activeParentMenu = "user-profile-page"
	- var activeMenu = "user-profile-page"

block breadcrumbs
	+breadcrumbs([{url:"index.html",name:"Home"}, {url:"#",name:"Pages"}, {name:"User Profile Page"}])

//- Include page content in page block
append content
	include ../contents/user-profile-page.html
	include ../contents/sidebar/right-sidebar.html
	include ../contents/sidebar/fab-menu.html


append pagecss
	link(rel='stylesheet', type='text/css', href=app_assets_path + '/css'+rtl+'/pages/user-profile-page.css')

//- Vendor JS
//------------------------------
//- Add vendor specific JS
append vendorjs

//- Page JS
//------------------------------
//- Add custom page specific JS
append pagejs