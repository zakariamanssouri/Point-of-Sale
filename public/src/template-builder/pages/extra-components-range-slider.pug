extends template

block pageVars
	- var pageTitle = "Range Slider"
	- var pageSubTitle = ""
	- var description = "The standard font Material Design uses is Roboto. We have included the font files with our framework.."
	- var activeParentMenu = "extra-components"
	- var activeMenu = "extra-components-range-slider"

block breadcrumbs
	+breadcrumbs([{url:"index.html",name:"Home"}, {url:"#",name:"Extra Components"}, {name:"Range Slider"}])

//- Include page content in page block
append content
	include ../contents/extra-components-range-slider.html
	include ../contents/sidebar/right-sidebar.html
	include ../contents/sidebar/fab-menu.html

//- Vendor css
//- Add vendor specific CSS
append vendorcss
	link(rel='stylesheet', type='text/css', href=app_assets_path + '/vendors/ionRangeSlider/css/ion.rangeSlider.css')
	link(rel='stylesheet', type='text/css', href=app_assets_path + '/vendors/ionRangeSlider/css/ion.rangeSlider.skinFlat.css')

//- Vendor JS
//------------------------------
//- Add vendor specific JS
append vendorjs
	script(src=app_assets_path + '/vendors/ionRangeSlider/js/ion.rangeSlider.js')

//- Page JS
//------------------------------
//- Add custom page specific JS
append pagejs
	script(src=app_assets_path + '/js/scripts/extra-components-range-slider.js')