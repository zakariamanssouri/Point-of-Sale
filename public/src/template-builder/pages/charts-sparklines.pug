extends template

block pageVars
	- var pageTitle = "Charts Sparklines"
	- var pageSubTitle = ""
	- var description = "The standard font Material Design uses is Roboto. We have included the font files with our framework."
	- var activeParentMenu = "chart"
	- var activeMenu = "charts-sparklines"

block breadcrumbs
	+breadcrumbs([{url:"index.html",name:"Home"}, {url:"#",name:"Charts"}, {name:"Charts Sparklines"}])

//- Include page content in page block
append content
	include ../contents/charts-sparklines.html
	include ../contents/sidebar/right-sidebar.html
	include ../contents/sidebar/fab-menu.html

//- Vendor JS
//------------------------------
//- Add vendor specific JS
append vendorjs
	script(src=app_assets_path + '/vendors/sparkline/jquery.sparkline.min.js')

//- Page JS
//------------------------------
//- Add custom page specific JS
append pagejs
	script(src=app_assets_path + '/js/scripts/charts-sparklines.js')

//- Page css
//- Add page specific CSS
append pagecss
	link(rel='stylesheet', type='text/css', href=app_assets_path + '/css/pages/charts-sparkline.css')