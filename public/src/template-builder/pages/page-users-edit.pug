extends template

block pageVars
	- var pageTitle = "Users edit"
	- var pageSubTitle = ""
	- var activeParentMenu = "user"
	- var activeMenu = "users-edit"

block breadcrumbs
	+breadcrumbs([{url:"index.html",name:"Home"}, {url:"#",name:"User"}, {name:"Users Edit"}])

//- Include page content in page block
append content
	include ../contents/page-users-edit.html
	include ../contents/sidebar/right-sidebar.html
	include ../contents/sidebar/fab-menu.html

//- Vendor JS
//------------------------------
//- Add vendor specific JS
append vendorjs
	script(src=app_assets_path + '/vendors/select2/select2.full.min.js')
	script(src=app_assets_path + '/vendors/jquery-validation/jquery.validate.min.js')    

//- Vendor css
//- Add vendor specific CSS
append vendorcss
	link(rel ='stylesheet' href=app_assets_path + '/vendors/select2/select2.min.css', type='text/css')
	link(rel ='stylesheet' href=app_assets_path + '/vendors/select2/select2-materialize.css', type='text/css')
//- Page JS
//------------------------------
//- Add custom page specific JS
append pagejs
	script(src=app_assets_path + '/js/scripts/page-users.js')

//- Page CSS
//------------------------------
//- Add custom page specific CSS
append pagecss
	link(rel='stylesheet', type='text/css', href=app_assets_path + '/css'+rtl+'/pages/page-users.css') 