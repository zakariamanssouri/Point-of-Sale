extends template

block pageVars
	- var pageTitle = "Timeline Page"
	- var pageSubTitle = ""
	- var activeParentMenu = "pages"
	- var activeMenu = "page-timeline"

block breadcrumbs
	+breadcrumbs([{url:"index.html",name:"Home"}, {url:"#",name:"Pages"}, {name:"Timeline Page"}])

//- Include page content in page block
append content
	include ../contents/page-timeline.html
	include ../contents/sidebar/right-sidebar.html
	include ../contents/sidebar/fab-menu.html

//- Vendor JS
//------------------------------
//- Add vendor specific JS
append vendorjs

//- Page JS
//------------------------------
//- Add custom page specific JS
append pagejs


//- Page CSS
//------------------------------
//- Add custom page specific CSS
append pagecss
	link(rel='stylesheet', type='text/css', href=app_assets_path + '/css'+rtl+'/pages/page-timeline.css')