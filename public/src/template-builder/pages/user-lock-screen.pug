extends template

block pageVars
	- var pageTitle = "User Lock Screen"
	- var pageSubTitle = ""
	- var description = "The standard font Material Design uses is Roboto. We have included the font files with our framework.."
	- var activeParentMenu = "user"
	- var activeMenu = "user-lock-screen"

append pageConfig
	- var contentLayout = '1-column'
	- var pageClass = 'forgot-bg'
	- var blankPage = true //- OPTIONS: true, false
	- var navbarFixed = false //- OPTIONS:- true, false

block breadcrumbs
	+breadcrumbs([{url:"index.html",name:"Home"}, {url:"#",name:"User"}, {name:"User Lock Screen"}])

//- Include page content in page block
append content
	include ../contents/user-lock-screen.html


//- Page CSS
//------------------------------
//- Add custom page specific CSS
append pagecss
	link(rel='stylesheet', type='text/css', href=app_assets_path + '/css'+rtl+'/pages/lock.css')

//- Vendor JS
//------------------------------
//- Add vendor specific JS
append vendorjs

//- Page JS
//------------------------------
//- Add custom page specific JS
append pagejs