extends template

block pageVars
	- var pageTitle = "Cards Advance"
	- var pageSubTitle = ""
	- var description = "The standard font Material Design uses is Roboto. We have included the font files with our framework.."
	- var activeParentMenu = "cards"
	- var activeMenu = "cards-advance"

block breadcrumbs
	+breadcrumbs([{url:"index.html",name:"Home"}, {url:"#",name:"Cards"}, {name:"Cards Advance"}])

//- Include page content in page block
append content
	include ../contents/cards-advance.html
	include ../contents/sidebar/right-sidebar.html
	include ../contents/sidebar/fab-menu.html

//- Vendor JS
//------------------------------
//- Add vendor specific JS
append vendorjs
	script(src=app_assets_path + '/vendors/sparkline/jquery.sparkline.min.js')
	script(src=app_assets_path + '/vendors/chartjs/chart.min.js')

//- Page JS
//------------------------------
//- Add custom page specific JS
append pagejs
	script(src=app_assets_path + '/js/scripts/card-advanced.js')